<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.04103967" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.0729927" />

    <ImageButton
        android:id="@+id/btn_guitar"
        android:layout_width="178dp"
        android:layout_height="84dp"
        android:layout_marginTop="8dp"
        android:scaleType=""
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.146"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline2"
        app:layout_constraintVertical_bias="0.065"
        app:srcCompat="@drawable/guitar" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="128dp"
        android:layout_height="133dp"
        android:scaleType=""
        app:srcCompat="@drawable/drums"
        tools:layout_editor_absoluteX="267dp"
        tools:layout_editor_absoluteY="96dp" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="227dp"
        android:layout_height="58dp"
        android:scaleType=""
        app:srcCompat="@drawable/keyboard"
        tools:layout_editor_absoluteX="33dp"
        tools:layout_editor_absoluteY="199dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="197dp"
        android:layout_height="24dp"
        android:text="Start Your Rock Band!"
        android:textAlignment="center"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/guideline2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.571" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="137dp"
        android:layout_height="242dp"
        android:scaleType=""
        app:srcCompat="@drawable/singer"
        tools:layout_editor_absoluteX="258dp"
        tools:layout_editor_absoluteY="264dp" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="220dp"
        android:layout_height="44dp"
        android:scaleType=""
        app:srcCompat="@drawable/guitarelectric"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="274dp" />

    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="201dp"
        android:layout_height="181dp"
        android:scaleType=""
        app:srcCompat="@drawable/silhouette"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="332dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="violin"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="570dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="piano"
        tools:layout_editor_absoluteX="161dp"
        tools:layout_editor_absoluteY="570dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="trumpet"
        tools:layout_editor_absoluteX="277dp"
        tools:layout_editor_absoluteY="570dp" />

    <Button
        android:id="@+id/button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="flute"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="636dp" />

    <Button
        android:id="@+id/button5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="tuba"
        tools:layout_editor_absoluteX="161dp"
        tools:layout_editor_absoluteY="636dp" />

    <Button
        android:id="@+id/button6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="harmonica"
        tools:layout_editor_absoluteX="270dp"
        tools:layout_editor_absoluteY="636dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="113dp"
        android:layout_height="26dp"
        android:text="Extra Instruments"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/guideline3"
        tools:layout_editor_absoluteY="523dp" />

</androidx.constraintlayout.widget.ConstraintLayout>
